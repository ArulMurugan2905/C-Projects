#include <stdio.h>

int main() {
    int heights[50];
    int i, n;
    int sum = 0;

    printf("Enter the number of Elements: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Heights[%d] = ", i);
        scanf("%d", &heights[i]);
        sum = sum + heights[i];
    }

    printf("Sum: %d\n", sum);
    printf("Avg: %f\n", ((float)sum / n));

    return 0;
}
