#include <stdio.h>  
int main() 
{
    int Year; 

    printf("Enter the Year: ");  
    scanf("%d", &Year);


    if (Year % 100 == 0)  
    {
        if (Year % 400 == 0)  
        {
            printf("It's a Leap Year\n");  
        }
        
        else
        {
            printf("It's not a Leap Year\n"); 
        }
        
    }
    else 
    {
        if (Year % 4 == 0)
        {
            printf("It's a Leap Year\n");
        }
        
        else
        {
            printf("It's not a Leap Year\n");  
        }
        
    }

    return 0; 
}
